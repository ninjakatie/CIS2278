package hw1;

public class Dog {
	public String name;
	public String breed;
	public int age;
	public char gender;
	public int weight;
	public int id;
	
	public Dog(String name, String breed, int age, char gender, int weight, int id) {
		this.name = name;
		this.breed = breed;
		this.age = age;
		this.gender = gender;
		this.weight = weight;
		this.id = id;
	}
	
	public Dog() {
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getBreed() {
		return breed;
	}

	public void setBreed(String breed) {
		this.breed = breed;
	}

	public int getAge() {
		return age;
	}

	public void setAge(int age) {
		this.age = age;
	}

	public char getGender() {
		return gender;
	}

	public void setGender(char gender) {
		this.gender = gender;
	}

	public int getWeight() {
		return weight;
	}

	public void setWeight(int weight) {
		this.weight = weight;
	}

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	
}
